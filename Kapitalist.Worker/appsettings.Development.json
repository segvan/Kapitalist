{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Debug"
      }
    ]
  },
  "PlatformSettings": {
    "SupportedCurrencies": ["PLN","USD"],
    "JobFrequencySeconds": 5
  }
}
